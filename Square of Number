import matplotlib.pyplot as plt
empty = []
for i in range(1 , 1001):
    square = i**2
    empty.append(square)
print(empty)
plt.plot(empty)
plt.title('Square of Number')
plt.xlabel('Number')
plt.ylabel('Square')


Or use
def square(x , y):
   for i in range(x , y):
      square = i**2
      empty.append(square)
print(square(x , y))
